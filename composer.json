{
  "name": "level23/dynadot-api",
  "type": "library",
  "description": "Implementation of the Dynadot API",
  "keywords": [
    "dynadot"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Teye Heimans",
      "role": "Developer",
      "email": "teye@level23.nl"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  },
  "require-dev": {
    "phpunit/phpunit": "^12",
    "monolog/monolog": "^2|^3",
    "phpstan/phpstan": "^2"
  },
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": ">=6.0",
    "psr/log": "^2.0|^3.0",
    "sabre/xml": "^2"
  },
  "autoload": {
    "psr-4": {
      "Level23\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Level23\\Dynadot\\Tests\\": "tests/"
    }
  }
}
